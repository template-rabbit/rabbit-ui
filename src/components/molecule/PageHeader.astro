---
import Heading from "../atom/Heading.astro";
export interface Props {
  title: text
  subTitle: text
}

const {title, subTitle} = Astro.props as Props;
---
<Heading level="1" mb="2" align="center" as="h3" weight="normal">{title}</Heading>
<Heading level="2" mb="12" align="center" as="h4" weight="thin" dim>{subTitle}</Heading>
