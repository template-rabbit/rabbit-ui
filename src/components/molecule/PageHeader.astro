---
import Heading from "../atom/Heading.astro";
export interface Props {
  title: string
  subTitle: string
}

const {title, subTitle} = Astro.props as Props;
---
<Heading level="1" mb="2" align="center" as="h2" weight="normal">{title}</Heading>
<Heading level="2" mb="12" align="center" as="h3" weight="light" dim>{subTitle}</Heading>
